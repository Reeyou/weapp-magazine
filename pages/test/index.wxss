.container {
  padding: 0 8rpx;
}
.pic-box {
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
}
.pic {
  width: 730rpx;
}
.title {
  font-size: 36rpx;
  width: 20em;
  letter-spacing: 4rpx;
  line-height: 72rpx;
  margin-top: 40rpx;
}
.tip-box {
  margin-top: 20rpx;
  text-align: right;
  font-size: 30rpx;
}